{
  "name": "openapi-frontend",
  "description": "JavaScript library for consuming OpenAPI-enabled APIs",
  "version": "0.1.1",
  "author": "Viljami Kuosmanen <viljami@avoinsorsa.fi>",
  "license": "MIT",
  "keywords": [
    "openapi",
    "swagger",
    "client",
    "frontend",
    "browser",
    "mock"
  ],
  "homepage": "https://github.com/anttiviljami/openapi-frontend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttiviljami/openapi-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/anttiviljami/openapi-frontend/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!scripts",
    "!node_modules",
    "!src",
    "!*.config.js"
  ],
  "dependencies": {
    "@types/swagger-parser": "^4.0.2",
    "axios": "^0.18.0",
    "bath": "^2.1.1",
    "lodash": "^4.17.11",
    "swagger-parser": "^6.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.9",
    "axios-mock-adapter": "^1.15.0",
    "jest": "^23.6.0",
    "markdown-toc": "^1.2.0",
    "openapi-types": "^1.3.2",
    "prettier": "^1.14.3",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "lint": "tslint --format prose --project .",
    "prettier": "prettier --write src/** __tests__/**",
    "toc": "./scripts/generate-toc.js",
    "test": "NODE_ENV=test jest"
  }
}
